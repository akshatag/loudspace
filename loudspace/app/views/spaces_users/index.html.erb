<p id="notice"><%= notice %></p>

<h1>Listing Spaces Users</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @spaces_users.each do |spaces_user| %>
      <tr>
        <td><%= link_to 'Show', spaces_user %></td>
        <td><%= link_to 'Edit', edit_spaces_user_path(spaces_user) %></td>
        <td><%= link_to 'Destroy', spaces_user, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Spaces user', new_spaces_user_path %>
